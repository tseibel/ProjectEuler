    "#print the difference between sum of squares and the square of sums\n",
    "sum_square = 0\n",
    "square_sum = 0\n",
    "sum = 0\n",
    "for i in range(0,101):\n",
    "    sum = sum + i\n",
    "    sum_square = sum_square + i*i\n",
    "    square_sum = sum*sum\n",
    "    \n",
    "print(sum_square, square_sum, square_sum - sum_square)"
